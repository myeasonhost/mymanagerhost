<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
<!--    <classPathEntry location="D:\tools\apache-maven-3.5.2\mysql\mysql-connector-java\5.1.30\mysql-connector-java-5.1.30.jar" />-->
    <context id="context1">
        <!-- 1.关闭自动生成注释(给生成文件的注释太多了,影响读程序) -->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!-- 2.连接数据库 -->
        <jdbcConnection connectionURL="jdbc:mysql://10.10.4.230:3306/myeason?serverTimezone=UTC&amp;useSSL=true" driverClass="com.mysql.jdbc.Driver" userId="root" password="eason" />
        <!-- 3.指定javabean生成位置 -->
        <javaModelGenerator targetPackage="com.eason.transfer.openapi.core.client.user.dao.entity" targetProject="./src/main/java" />
        <!-- 4.指定sql.mapper映射文件生成位置 -->
        <sqlMapGenerator targetPackage="com.eason.transfer.openapi.core.client.user.dao.mapper" targetProject="./src/main/java" />
        <!-- 5.指定sql.interface接口生成位置 -->
        <javaClientGenerator targetPackage="com.eason.transfer.openapi.core.client.user.dao.mapper" targetProject="./src/main/java" type="XMLMAPPER" />
        <!-- 6.table指定生成策略 -->
        <table tableName="t_user_info" domainObjectName="TUserInfo"></table>
    </context>
</generatorConfiguration>